<Window x:Class="FIFAModdingUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FIFAModdingUI"
        mc:Ignorable="d"
        Title="paulv2k4 FIFA Modding UI 2020.2" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="380"></ColumnDefinition>
            <ColumnDefinition Width="20"></ColumnDefinition>
            <ColumnDefinition Width="380"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnEditor" Grid.Column="0" Click="btnEditor_Click">Editor</Button>
        <Button x:Name="btnLauncher" Grid.Column="2" Click="btnLauncher_Click">Launcher</Button>
    </Grid>
</Window>
