<UserControl x:Class="FIFAModdingUI.Pages.Common.Editor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FIFAModdingUI.Pages.Common"
             mc:Ignorable="d" 
             >
    <UserControl.Resources>
        <DataTemplate DataType="{x:Type DataObject}"></DataTemplate>
    </UserControl.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Content="Modded" Background="AliceBlue" FontWeight="Bold"></Label>
        <TreeView x:Name="TreeView1" Grid.Column="0" Grid.Row="1" Height="425" 
                  ScrollViewer.VerticalScrollBarVisibility="Visible"
                  ScrollViewer.HorizontalScrollBarVisibility="Visible"
                  >
        </TreeView>
        <Label Content="Vanilla (Do not edit)" Grid.Column="1"  Background="AliceBlue" FontWeight="Bold" ></Label>
        <TreeView x:Name="TreeViewOriginal" Grid.Column="1" Grid.Row="1" Height="425" 
                  ScrollViewer.VerticalScrollBarVisibility="Visible"
                  ScrollViewer.HorizontalScrollBarVisibility="Visible"></TreeView>
    </Grid>


</UserControl>
